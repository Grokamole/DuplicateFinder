<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DuplicateFinder" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/DuplicateFinder" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="$(#boost.lib)\libboost_filesystem-mgw48-sd-1_55.a" />
					<Add library="$(#boost.lib)\libboost_system-mgw48-sd-1_55.a" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/DuplicateFinder" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-std=c++11" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="$(#boost.lib)\libboost_system-mgw48-s-1_55.a" />
					<Add library="$(#boost.lib)\libboost_filesystem-mgw48-s-1_55.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add directory="$(#boost.include)" />
		</Compiler>
		<Linker>
			<Add directory="$(#boost.lib)" />
		</Linker>
		<Unit filename="duplicatefinder.cpp" />
		<Unit filename="duplicatefinder.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
